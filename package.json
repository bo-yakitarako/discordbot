{
    "name": "bodiscordjs",
    "version": "1.0.0",
    "description": "discord bot developed by bo-yakitarako with Node.js",
    "main": "index.js",
    "scripts": {
        "start": "node ./build/index.js",
        "debug": "yarn compile && node ./build/test.js",
        "compile": "tsc -p .",
        "compile:test": "tsc -p . --noEmit",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "typeorm:prod": "node ./node_modules/typeorm/cli.js",
        "migration:generate": "rm -rf ./build && tsc && typeorm migration:generate -n",
        "migration:run": "rm -rf ./build && tsc && typeorm migration:run",
        "prod:migration-generate": "typeorm migration:generate -c production -n",
        "prod:migration-run": "typeorm migration:run -c production"
    },
    "author": "bo-yakitarako",
    "license": "MIT",
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/dotenv": "^8.2.0",
        "@types/node": "^14.0.6",
        "@types/node-fetch": "^2.5.7",
        "@types/pg": "^7.14.3",
        "@types/ws": "^7.2.4",
        "@typescript-eslint/eslint-plugin": "^4.9.0",
        "@typescript-eslint/parser": "^4.9.0",
        "eslint": "^7.14.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.2.0",
        "ts-node": "^9.1.1",
        "typescript": "^3.9.3"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "date-fns-timezone": "^0.1.4",
        "dayjs": "^1.10.3",
        "discord.js": "^12.2.0",
        "dotenv": "^8.2.0",
        "global": "^4.4.0",
        "node-fetch": "^2.6.0",
        "pg": "^8.2.1",
        "pg-hstore": "^2.3.3",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.30"
    }
}
